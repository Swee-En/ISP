public class PrimeNumberChecker{
    public static int isPrime(int num) {

        int result = new int;

        for (int i=2; i<=num; i++) {
            if (num % i == 0) {
                result = 0;
                break;
            }

            else {
                result = 1;
            }

        }

        return result;

    }
}
